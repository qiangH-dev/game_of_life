         {
          "name": "MultiplySim",
          "priority": 0,
          "accepted_tickmodes": ["Simulation"],
          "entity_agent_extension":{
            "accepted_names":["Cell"],
            "sim_model": {
             "providers": [
              {
               "dll_path": "GAMELIFE/MultiplyQtSim",
               "arg_options":[
               ],
               "sim_options": {
                "MultiplySim": {
                 "coroutine": true,
                 "display_name": "MultiplySim"
                }
               }
              }
             ]
            }
          }
         }
         ]
        },
        "bbclient_config": {
            "ticktock_verbose": false,
            "descriptor_pack_maxcount": 0
        }
