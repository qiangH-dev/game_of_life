    "enable_launcher": true,
    "launcher_config":{
        "agent_config": {
         "coroutine": true,
         "agents": [
         {
          "name": "GAMELIFEInitQt",
          "priority": 100,
          "provider_name": "GAMELIFE/GAMELIFEInitQtAgent",
          "accepted_tickmodes": ["DirectEditing"]
